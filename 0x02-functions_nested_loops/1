#include "main.h"
/**
  *print_sign - Prints the sign of a number
  *
  *@n: A letter in the ASCII table
  *
  *Return: 1 if n is greater than 0 and otherwise return 0
  *
  */
int print_sign(int n)
{
	if (n > 48)
	{
		_putchar(43);
		return (1);
	}
	else if (n = 48)
	{
		_putchar('0');
		return (0);
	}
	else 
	{
		_putchar(45);
		return (-1);
	}
	_putchar('\n');
}
